{
  "name": "deploy-your-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm --filter deploy-your-app-frontend dev\" \"pnpm --filter deploy-your-app-server dev\"",
    "dev:server": "pnpm --filter deploy-your-app-server dev",
    "build": "pnpm --filter deploy-your-app-server build && pnpm --filter deploy-your-app-frontend build",
    "build:frontend": "pnpm --filter deploy-your-app-frontend build",
    "deploy:pages": "pnpm build:frontend && gh-pages -d frontend/dist -b gh-pages",
    "lint": "eslint .",
    "preview": "pnpm --filter deploy-your-app-frontend preview"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "concurrently": "^9.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4"
  },
  "resolutions": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
